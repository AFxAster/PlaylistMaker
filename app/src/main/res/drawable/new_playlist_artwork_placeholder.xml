<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="@dimen/new_playlist_artwork_size"
        android:height="@dimen/new_playlist_artwork_size">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="@color/YP_text_gray"
                android:dashWidth="32dp"
                android:dashGap="32dp" />
            <corners android:radius="8dp" />
        </shape>
    </item>

    <item
        android:width="80dp"
        android:height="80dp"
        android:drawable="@drawable/ic_add_image"
        android:gravity="center" />
</layer-list>